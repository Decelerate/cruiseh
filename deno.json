{
  "name": "@decelerate/cruiseh",
  "version": "0.1.2",
  "license": "MIT",
  "exports": {
    ".": "./index.ts",
    "./router": "./router.ts",
    "./types": "./types.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "CONTRIBUTING.md",
      "deno.json",
      "index.ts",
      "router.ts",
      "types.ts"
    ]
  },
  "tasks": {
    "doc:gen": "deno doc --html --name=\"Cruiseh API\" ./index.ts",
    "doc:dev": "deno run -A docs-dev-server.ts",
    "doc:lint": "deno doc --lint index.ts",
    "fmt": "deno fmt",
    "fmt:check": "deno fmt --check",
    "check": "deno check ."
  },
  "fmt": {
    "exclude": [
      "README.md"
    ]
  },
  "lock": false
}
